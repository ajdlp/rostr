<p>Event has started. Show all groups</p>

<div id="group_list">
<link href='http://fonts.googleapis.com/css?family=Roboto:400,100' rel='stylesheet' type='text/css'>
  <ol>
  <% @groups.each do |group| %>
      <li>
      <%= group.name %>
      <% @students.each do |student| %>
        <% if(student.group_id == group.id) %>
          <ul><%= student.name %></ul>
        <% end  %>
      <% end  %>
      </li>
  <% end %>
  </ol>
</div>
