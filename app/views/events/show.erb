
<h1><%= @event.title %></h1>

<% @event.groups.each do |group| %>
  <%= group.name %>
  <form action="/events/<%=@event.id%>/groups/<%=group.id%>" method = "post">
    <input type = "text" name="password" placeholder="Email">
    <input type="submit" value="Enter Group">

  </form>
<%end%>

<%= @incorrect_password %>
